{% extends 'base.html.twig' %}

{% block title %}Domaine{% endblock %}

{% block body %}
    <h1>Domaine</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ domaine.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ domaine.titre }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ domaine.image }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_domaine_index') }}">Retour vers la liste</a>

    <a href="{{ path('app_domaine_edit', {'id': domaine.id}) }}">Modifier</a>

    {{ include('admin/domaine/_delete_form.html.twig') }}
{% endblock %}
